{
  "name": "staking-pool",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node -r esm ./bin/www",
    "start:mon": "nodemon -r esm ./bin/www",
    "start:debug": "DEBUG=express:* node -r esm ./bin/www",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development --watch",
    "start:dev": "concurrently \"yarn run start:mon\" \"yarn run build:dev\""
  },
  "dependencies": {
    "blockui": "^1.0.0",
    "bootstrap": "^4.4.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "~4.16.1",
    "express-translate": "^0.4.0",
    "express-webpack-assets": "^0.1.0",
    "http-errors": "~1.6.3",
    "jquery": "^3.4.1",
    "jquery-animated-headlines": "^1.0.0",
    "magnific-popup": "^1.1.0",
    "morgan": "~1.9.1",
    "nodemailer": "^6.4.6",
    "owl.carousel": "^2.3.4",
    "particles.js": "^2.0.0",
    "popper.js": "^1.16.0",
    "pug": "~3.0.1",
    "simplebar": "^5.1.0",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@webpack-cli/init": "^0.3.0",
    "assets-webpack-plugin": "^3.9.12",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "css-loader": "^3.4.2",
    "fibers": "^4.0.2",
    "file-loader": "^6.0.0",
    "inquirer": "^6.0.0",
    "inquirer-autocomplete-prompt": "^1.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.3",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": [
    "defaults"
  ],
  "nodemonConfig": {
    "verbose": false,
    "ext": "js,json",
    "ignore": ["assets/*", "public/*"],
    "delay": 2500
  }
}
